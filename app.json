{
  "expo": {
    "name": "mugicians",
    "slug": "mugicians",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "android": {
      "package": "com.ananun.mugicians",
      "versionCode": 3,
      "permissions": [
        "RECORD_AUDIO"
      ]
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "A songwriter notebook. Organize your song ideas and thoughts",
    "githubUrl": "https://github.com/anAnun/mugicians-mobile"
  }
}